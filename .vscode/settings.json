{
    "workbench.colorTheme": "Default Dark+",
    "cmake.configureOnOpen": true,
    "window.zoomLevel": 0.0,
    "editor.minimap.enabled": false,
    "C_Cpp.autocomplete": "default",
    "[cpp]": {
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "[c]": {
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "C_Cpp.errorSquiggles": "disabled"
}
